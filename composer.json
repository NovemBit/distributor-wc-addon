{
	"name": "novembit/distributor-addon-template",
	"description": "Template factory for creating distributor addons",
	"authors": [
		{
			"name": "Novembit",
			"email": "info@novembit.com"
		}
	],
	"type": "wordpress-plugin",
	"license": "GPL-3.0-only / GPL-3.0-or-later",
	"require": {
		"php": ">=5.6",
		"yahnis-elsts/plugin-update-checker": "^4.4"
	},
	"require-dev": {
		"10up/phpcs-composer": "dev-master"
	},
	"scripts": {
		"lint": "./vendor/bin/phpcs . --runtime-set testVersion 5.6-",
		"lint-fix": "phpcbf . --runtime-set testVersion 5.6-"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
